; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Color Spike"
run/main_scene="res://Scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

Globals="*res://Globals.gd"
Globalnode="*res://Scenes/Globalnode.tscn"

[display]

window/size/width=750
window/size/height=1334
window/size/test_width=750
window/size/test_height=1334
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[rendering]

quality/driver/driver_name="GLES2"
quality/driver/fallback_to_gles2=true
quality/2d/gles2_use_nvidia_rect_flicker_workaround=true
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
